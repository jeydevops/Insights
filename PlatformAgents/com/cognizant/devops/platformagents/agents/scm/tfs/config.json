{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg":"iAgent"
  },
  "subscribe": {
    "config": "SCM.TFS.config"
  },
  "publish": {
    "data": "SCM.TFS.DATA",
    "health": "SCM.TFS.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "timeFieldMapping" : {
		"startDate" : "%Y-%m-%d"
  	  },
	  "responseTemplate": {
	    "changesetId": "changesetId",
	    "createdDate": "commitDate",
	    "comment": "comment",
	    "checkedInBy": {
	      "displayName": "scmAuthorName",
	      "uniqueName": "scmAuthor"
	    },
	    "author": {
	      "displayName": "creatorName",
	       "uniqueName": "creator"
	    }
	  } 
  },
"agentId": "",

"auth": "NTLM",
"toolCategory" : "SCM",
"toolsTimeZone" : "Asia/Kolkata",
"insightsTimeZone" : "Asia/Kolkata",
"useResponseTemplate" : true,
"enableValueArray": false,
"enableDataValidation": true,
"runSchedule": 2,
"userID": "tfs",
"passwd": "tfs",
"baseUrl": "http://hostname/tfs",
"timeStampField":"commitDate",
"timeStampFormat":"%Y-%m-%dT%H:%M:%S",
"isEpochTimeFormat" : false,

"startFrom" : "2017-01-01 01:01:01",
"isDebugAllowed" : false,
"loggingSetting" : {
		"logLevel" : "WARN",
		"maxBytes" : 5000000,
		"backupCount" : 1000
	}
}